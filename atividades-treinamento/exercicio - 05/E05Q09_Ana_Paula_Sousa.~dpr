program E05Q09_Ana_Paula_Sousa;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var
  fNumA, fNumB, fMedia : Double;

begin

Writeln('Preencha com duas notas: ');
Readln(fNumA);
Readln(fNumB);

fMedia := (fNumA+fNumB)/2;

if fMedia >= 5.0 then
  WriteLn('Voce foi: APROVADO')
else
  WriteLn('Voce foi: REPROVADO');


Readln;
end.
